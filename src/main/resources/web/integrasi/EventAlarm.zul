<div xmlns:n="native" id="eventAlarmPanel" apply="${eventAlarmController}">
<n:main class="eventAlarm">
	<n:h1>Laporan Event / Alarm dari PLBN</n:h1>
	<div style="padding-top: 10px;">
		<div style="display: flex; gap: 10px; align-items: center;">
			<n:h2>PLBN:</n:h2>
			<combobox id="plbnCombobox" value="ENTIKONG" width="200px"></combobox>
		</div>		
	</div>
	<div class="twoColumns">
		<div style="padding-top: 10px;">
			<listbox id="eventAlarmListbox" height="360px" mold="paging" pageSize="2">
				<listhead>
					<listheader label="Tgl.Jam Event/Alarm" width="140px"></listheader>
